```java
	public static void merge(Comparable[] a, int lo, int mid, int hi){
		assert isSorted(a,lo,mid);
		assert isSorted(a,mid+1,hi);
		aux = new Comparable[a.length];
		for (int k=lo; k<=hi; k++) {
			aux[k] = a[k];
		}
		int i = lo; int j = mid+1;
		for (int k=lo; k<=hi; k++) {
			if (i<= mid && j<=hi) {
				if (less(aux[i],aux[j])) {
					a[k] = aux[i];
					i++;
				}
				else {
					a[k] = aux[j];
					j++;
				}
			}
			else if (i>mid && j <= hi){
				a[k] = aux[j];
				j++;
			}
			else if (i <= mid && j > hi){
				a[k] = aux[i];
				i++;
			}
		}
		assert isSorted(a,lo,hi);
	}
